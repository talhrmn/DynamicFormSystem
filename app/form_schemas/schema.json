{
  "name": {
    "type": "string",
    "required": true,
    "minLength": 3
  },
  "email": {
    "type": "email",
    "required": true
  },
  "password": {
    "type": "password",
    "required": true
  },
  "age": {
    "type": "number",
    "required": true,
    "min": 0
  },
  "gender": {
    "type": "dropdown",
    "options": [
      "male",
      "female",
      "other"
    ],
    "required": true
  }
}